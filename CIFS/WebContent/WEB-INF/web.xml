	</init-param>	
	<init-param>
		<param-name>url</param-name>
		<param-value>jdbc:odbc:test</param-value>
	</init-param>	
	<init-param>
		<param-name>user</param-name>
		<param-value>cis</param-value>
    </init-param>	
